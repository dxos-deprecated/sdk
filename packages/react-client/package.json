    "@dxos/metrics": "^1.0.0-beta.6",
    "@dxos/random-access-multi-storage": "^1.1.0-beta.4",
    "level-js": "^5.0.1",
    "memdown": "^5.1.0",
    "pify": "^4.0.1",
    "react-display-name": "^0.2.5",
    "react-router-dom": "^5.1.2",
    "use-deep-compare-effect": "^1.3.0"
  },
  "devDependencies": {
