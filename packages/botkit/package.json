    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-inline-import": "^3.0.0",
    "jest": "^25.1.0",
    "semistandard": "14.2.0"
  },
  "semistandard": {
    "parser": "babel-eslint",
    "env": [
      "jest",
      "node",
      "browser"
    ],
    "ignore": [
      "dist"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
