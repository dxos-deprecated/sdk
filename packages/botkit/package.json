    "lodash.defaultsdeep": "^4.6.1",
    "lodash.get": "^4.4.2",
    "lodash.isempty": "^4.4.0",
    "memdown": "^5.1.0",
    "moment": "^2.24.0",
    "node-ipc": "^9.1.1",
    "node-watch": "^0.6.4",
    "random-access-memory": "^3.1.1",
