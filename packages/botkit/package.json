  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && npm run build:protobuf && npm run dist",
    "build:protobuf": "pbjs -t json src/ipc/schema.proto -o src/ipc/schema.json && cpy src/ipc/schema.json dist/ipc",
    "build:watch": "yarn run build --watch",
    "clean": "rm -rf dist",
    "dist": "babel ./src --out-dir ./dist --ignore \"**/*.test.js\"",
    "lint": "semistandard",
    "prepublishOnly": "npm run build && npm run test",
    "test": "jest --rootDir ./src --verbose --passWithNoTests"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "@dxos/async": "^1.0.0-beta.2",
