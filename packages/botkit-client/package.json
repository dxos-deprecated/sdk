{
  "name": "@dxos/botkit-client",
  "version": "1.0.0-beta.135",
  "description": "Botkit client",
  "keywords": [],
  "license": "GPL-3.0",
  "author": "DXOS.org",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && npm run dist",
    "build:watch": "yarn run build --watch",
    "clean": "rm -rf dist",
    "dist": "babel ./src --out-dir ./dist --ignore \"**/*.test.js\"",
    "lint": "semistandard",
    "prepublishOnly": "npm run build && npm run test",
    "test": "jest --rootDir ./src --verbose --passWithNoTests"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "@dxos/async": "^1.0.0-beta.1",
    "@dxos/client": "^1.0.0-beta.135",
    "@dxos/crypto": "^1.0.0-beta.1",
    "@dxos/debug": "^1.0.0-beta.73",
    "@dxos/network-manager": "^1.0.0-beta.94",
    "@dxos/protocol-plugin-bot": "^1.1.0-beta.12",
    "assert": "^2.0.0",
    "debug": "^4.1.1",
    "hypercore-crypto": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "jest": "^25.1.0",
    "semistandard": "14.2.0"
  },
  "semistandard": {
    "parser": "babel-eslint",
    "env": [
      "jest",
      "node",
      "browser"
    ],
    "ignore": [
      "dist"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
