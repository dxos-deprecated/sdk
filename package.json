{
  "name": "@dxos/sdk",
  "version": "2.7.5",
  "private": true,
  "description": "DXOS SDK monorepo.",
  "homepage": "https://github.com/dxos/sdk#readme",
  "bugs": {
    "url": "https://github.com/dxos/sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxos/sdk.git"
  },
  "license": "AGPL-3.0",
  "workspaces": {
    "packages": [
      "packages/*",
      "bot/*"
    ]
  },
  "scripts": {
    "build": "wsrun -t build",
    "clean": "wsrun clean",
    "lint": "wsrun lint",
    "test": "wsrun -s test",
    "coverage": "istanbul-combine -d coverage -p summary -r lcov packages/**/coverage/coverage-final.json",
    "lint:lockfile": "lockfile-lint --path yarn.lock --allowed-hosts yarn npm codeload.github.com --validate-https"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "istanbul-combine": "^0.3.0",
    "lerna": "^3.19.0",
    "lint-staged": "^9.5.0",
    "lockfile-lint": "^4.3.7",
    "semistandard": "14.2.0",
    "wsrun": "^5.2.4"
  },
  "semistandard": {
    "parser": "babel-eslint",
    "env": [
      "jest",
      "node",
      "browser"
    ]
  }
}
